
@{
    ViewBag.Title = "Index";
}

<div class="jumbotron">
    <h1>Macro Tracker</h1>
    <p class="lead"><p>Track your nutrtion for each day.  Use your own nutrition information or search the web!</p>

    <form>
        <div class="form-group">
            <label>UserName</label>
            <input type="email" class="form-control" id="username" placeholder="Enter User">
        </div>


        <div class="form-group">
            <label>Password</label>
            <input type="password" class="form-control" id="password" placeholder="Enter Password">
        </div>


        <button type="submit" class="btn btn-primary" value="Food">Submit</button>

    </form>

</div>

